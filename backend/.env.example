# === External APIs ===
ETHERSCAN_API_KEY=your-etherscan-key
GOPLUS_API_KEY=

# === Redis ===
REDIS_URL=redis://localhost:6379/0

# === Rate Limiting ===
RATE_LIMIT_PER_MINUTE=60
